{
  "name": "disord-bot",
  "author": "CÃ©dric Meyer",
  "version": "1.0.0",
  "description": "a multipurpose discord bot",
  "license": "MIT",
  "main": "src/main.ts",
  "scripts": {
    "start": "ts-node src/main.ts",
    "watch": "nodemon"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src",
      "src/discords",
      "src/quards"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.3",
    "@typeit/discord": "^4.0.8",
    "discord.js": "^12.4.1",
    "ffmpeg": "0.0.4",
    "ffmpeg-static": "^4.2.7",
    "fluent-ffmpeg": "^2.1.2",
    "node-schedule": "^2.0.0",
    "sequelize": "^6.3.5",
    "sqlite3": "^5.0.0",
    "twitch-js": "^2.0.0-beta.42",
    "@types/node": "^14.14.6",
    "ts-node": "^9.0.0",
    "ytdl-core": "^4.0.0",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@types/node-schedule": "^1.3.1",
    "codelyzer": "^6.0.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  }
}
